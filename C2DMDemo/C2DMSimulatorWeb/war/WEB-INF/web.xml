<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>RegisterServlet</servlet-name>
		<servlet-class>fr.prados.android.c2dm.web.RegisterServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>RegisterServlet</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SendServlet</servlet-name>
		<servlet-class>fr.prados.android.c2dm.web.SendServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SendServlet</servlet-name>
		<url-pattern>/send</url-pattern>
	</servlet-mapping>
 		
	<servlet>
		<servlet-name>ClientLoginServlet</servlet-name>
		<servlet-class>fr.prados.android.c2dm.web.ClientLoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ClientLoginServlet</servlet-name>
		<url-pattern>/ClientLogin</url-pattern>
	</servlet-mapping>
		
	<servlet>
		<servlet-name>PoolServlet</servlet-name>
		<servlet-class>fr.prados.android.c2dm.web.PoolServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PoolServlet</servlet-name>
		<url-pattern>/pool</url-pattern>
	</servlet-mapping>

	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
